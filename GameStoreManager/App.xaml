<Application x:Class="GameStoreManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:GameStoreManager"
             xmlns:viewModel="clr-namespace:GameStoreManager.Client.ViewModels"
             xmlns:view="clr-namespace:GameStoreManager.Client.Views">
    <Application.Resources>
        <DataTemplate DataType="{x:Type TypeName=viewModel:MainViewModel}">
            <view:MainWindow/>
        </DataTemplate>
    </Application.Resources>
</Application>
